{
	"name": "pixelpizza",
	"version": "2.0.0-alpha",
	"description": "The pixel pizza bot. Currently developing using Sapphire",
	"main": "./dist/index.js",
	"directories": {
		"lib": "src",
		"test": "__tests__"
	},
	"files": [
		"dist"
	],
	"scripts": {
		"test": "yarn build && jest --pass-with-no-tests",
		"test:ci": "jest --no-stack-trace --verbose --pass-with-no-tests",
		"build": "yarn clean && tsup",
		"build:prod": "yarn build && rollup -c scripts/rollup.prod.ts",
		"build:dev": "yarn build && rollup -c scripts/rollup.dev.ts",
		"clean": "node scripts/clean-dist.mjs",
		"lint": "eslint src scripts --ext mjs,js,ts --fix",
		"format": "prettier --write **/*.{ts,js,json,yml,yaml}",
		"prepare": "husky install",
		"changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
		"start:prod": "yarn build:prod && node dist/index.js",
		"start:dev": "yarn build:dev && node dist/index.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/discordextend/discord-oversimplified.git"
	},
	"keywords": [
		"discord-extend",
		"discordjs",
		"discord"
	],
	"author": "Jaron Zijlstra",
	"license": "Apache-2.0",
	"bugs": {
		"url": "https://github.com/discordextend/discord-oversimplified/issues"
	},
	"homepage": "https://github.com/discordextend/discord-oversimplified#readme",
	"dependencies": {
		"@devtomio/plugin-botlist": "^1.2.0",
		"@discordjs/builders": "^0.12.0",
		"@kaname-png/plugin-env": "^1.1.0",
		"@kaname-png/plugin-statcord": "^1.3.2",
		"@prisma/client": "^3.11.1",
		"@sapphire/decorators": "^4.0.2",
		"@sapphire/framework": "^3.0.0-next.18259a7.0",
		"@sapphire/plugin-api": "^3.2.1",
		"@sapphire/plugin-logger": "^2.0.2",
		"@sapphire/time-utilities": "^1.7.4",
		"common-tags": "^1.8.0",
		"discord-api-types": "^0.26.1",
		"discord-oversimplified": "^0.6.5-next.d3e32eb.0",
		"discord.js": "^13.6.0",
		"tslib": "^2.3.1",
		"valid-url": "^1.0.9"
	},
	"devDependencies": {
		"@babel/core": "7.17.10",
		"@babel/plugin-proposal-decorators": "7.17.9",
		"@babel/preset-env": "7.17.10",
		"@babel/preset-typescript": "7.16.7",
		"@commitlint/cli": "16.2.4",
		"@commitlint/config-angular": "16.2.4",
		"@types/common-tags": "1.8.1",
		"@types/jest": "27.5.0",
		"@types/node": "17.0.18",
		"@types/require-all": "3.0.3",
		"@types/valid-url": "1.0.3",
		"@typescript-eslint/eslint-plugin": "5.12.0",
		"@typescript-eslint/parser": "5.12.0",
		"babel-plugin-const-enum": "1.2.0",
		"babel-plugin-transform-typescript-metadata": "0.3.2",
		"conventional-changelog-cli": "2.2.2",
		"eslint": "8.15.0",
		"eslint-config-marine": "9.3.2",
		"eslint-config-prettier": "8.5.0",
		"eslint-plugin-import": "2.26.0",
		"eslint-plugin-prettier": "4.0.0",
		"husky": "8.0.0",
		"jest": "28.1.0",
		"prettier": "2.6.2",
		"prisma": "3.13.0",
		"rollup": "2.72.1",
		"rollup-plugin-inject-process-env": "1.3.1",
		"rollup-plugin-uglify": "6.0.4",
		"tsup": "5.12.7",
		"typescript": "4.5.5"
	}
}
